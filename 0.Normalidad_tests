    "\n",
    "def lanzar_dado(array, n_size = 5, bootstrap_size = len(medias)):\n",
    "    for k in range(bootstrap_size):\n",
    "        array[k] = np.mean(np.random.randint(1,7,size=n_size))\n",
    "    return array\n",
    "\n",
    "resultados = lanzar_dado(medias)"
   ]
